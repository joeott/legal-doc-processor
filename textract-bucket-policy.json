{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowTextractAccess",
      "Effect": "Allow",
      "Principal": {
        "Service": "textract.amazonaws.com"
      },
      "Action": [
        "s3:GetObject",
        "s3:GetObjectVersion"
      ],
      "Resource": "arn:aws:s3:::samu-docs-private-upload/*",
      "Condition": {
        "StringEquals": {
          "aws:SourceAccount": "371292405073"
        }
      }
    }
  ]
}